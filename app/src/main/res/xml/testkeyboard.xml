<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"


    android:verticalGap="0px"
    android:keyHeight="45dp"
    >
    <Row android:keyboardMode="@integer/keyboard_normal">
        <Key android:codes="97"
            android:keyEdgeFlags="left" android:keyIcon="@drawable/alpha_a_vertical"/>
        <Key android:codes="101" android:keyLabel="e"/>
        <Key android:codes="98" android:keyLabel="b"/>
        <Key android:codes="102" android:keyLabel="f"/>
        <Key android:codes="103" android:keyLabel="g"/>
        <Key android:codes="108" android:keyLabel="l" />
        <Key android:codes="109" android:keyLabel="m"/>
        <Key android:codes="114" android:keyLabel="r"/>
        <Key android:codes="118" android:keyLabel="v"/>
        <Key android:codes="119" android:keyLabel="w"/>
    </Row>

    <Row android:keyboardMode="@integer/keyboard_normal"  >
        <Key android:codes="105" android:keyLabel="i" android:horizontalGap="5%" android:keyEdgeFlags="left"/>
        <Key android:codes="111" android:keyLabel="o"/>
        <Key android:codes="99" android:keyLabel="c"/>
        <Key android:codes="104" android:keyLabel="h"/>
        <Key android:codes="110" android:keyLabel="n"/>
        <Key android:codes="112" android:keyLabel="p" />
        <Key android:codes="115"  android:keyLabel="s"/>
        <Key android:codes="120" android:keyLabel="x"/>
        <Key android:codes="121" android:keyLabel="y"/>
    </Row>

    <Row android:keyboardMode="@integer/keyboard_normal" android:keyBackground="@drawable/background_oped_orange">
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift_holo_dark"
            android:keyWidth="15%p"
            android:isSticky="true" android:keyEdgeFlags="left"/>

        <Key android:codes="117" android:keyLabel="u" />
        <Key android:codes="100" android:keyLabel="d"/>
        <Key android:codes="106" android:keyLabel="j"/>
        <Key android:codes="107" android:keyLabel="k"/>
        <Key android:codes="113" android:keyLabel="q" />
        <Key android:codes="116" android:keyLabel="t"/>
        <Key android:codes="122" android:keyLabel="z" />
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="15%p" android:keyEdgeFlags="right"
            android:isRepeatable="true"/>
    </Row>
    <Row android:keyboardMode="@integer/keyboard_symbol" >
        <Key android:codes="49" android:keyLabel="1"/>
        <Key android:codes="50" android:keyLabel="2"/>
        <Key android:codes="51" android:keyLabel="3"/>
        <Key android:codes="52" android:keyLabel="4" android:keyBackground="@drawable/background_oped_orange"/>
        <Key android:codes="53" android:keyLabel="5"/>
        <Key android:codes="54" android:keyLabel="6"/>
        <Key android:codes="55" android:keyLabel="7" />
        <Key android:codes="56" android:keyLabel="8"/>
        <Key android:codes="57" android:keyLabel="9"/>
        <Key android:codes="48" android:keyLabel="0" />
    </Row>

    <Row  android:keyboardMode="@integer/keyboard_symbol" >
        <Key android:codes="33" android:keyLabel="!" android:horizontalGap="5%p" android:keyEdgeFlags="left"/>
        <Key android:codes="34" android:keyLabel="&quot;" />
        <Key android:codes="39" android:keyLabel="'" />
        <Key android:codes="58" android:keyLabel=":" />
        <Key android:codes="59" android:keyLabel=";" />
        <Key android:codes="47" android:keyLabel="/" />
        <Key android:codes="63" android:keyLabel="\?" />
        <Key android:codes="64" android:keyEdgeFlags="right" android:keyLabel="\@"
            />
        <Key android:codes="35" android:keyLabel="\#" />

    </Row>
    <Row  android:keyboardMode="@integer/keyboard_symbol" >


        <Key android:codes="36" android:keyLabel="$" android:keyEdgeFlags="left" android:horizontalGap="1%p"/>
        <Key android:codes="37" android:keyLabel="%" />
        <Key android:codes="38" android:keyLabel="&amp;" />
        <Key android:codes="42" android:keyLabel="*" />
        <Key android:codes="45" android:keyLabel="-" />
        <Key android:codes="61" android:keyLabel="=" />
        <Key android:codes="40" android:keyLabel="(" android:keyIcon="@android:drawable/ic_menu_search"/>
        <Key android:codes="41" android:keyEdgeFlags="right" android:keyLabel=")" />
        <Key android:codes="-5" android:keyLabel="del"
            android:keyWidth="15%p" android:keyEdgeFlags="right"
            android:isRepeatable="true"/>
    </Row>


    <Row android:rowEdgeFlags="bottom" android:keyboardMode="@integer/keyboard_normal" >

        <Key android:codes="-2"  android:keyWidth="20%p"   android:keyIcon="@drawable/sym_keyboard_zwj_holo_dark"/>
        <Key android:codes="-2" android:keyLabel="123" android:keyWidth="15%p"  />

        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space_holo_dark"
            android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="46,44" android:keyLabel=". ,"
            android:keyWidth="15%p"/>
        <Key android:codes="-4" android:keyIcon="@drawable/sym_keyboard_feedback_return"
            android:keyWidth="25%p"/>
    </Row>
    <Row android:rowEdgeFlags="bottom" android:keyboardMode="@integer/keyboard_symbol">
        <Key android:codes="-2"  android:keyWidth="20%p"   android:keyIcon="@drawable/sym_keyboard_zwj_holo_dark"/>
        <Key android:keyWidth="14.999998%p" android:codes="-2" android:keyLabel="ABC" android:keyboardMode="@integer/keyboard_symbol"/>

        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space_holo_dark"
            android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="46,44" android:keyLabel=". ,"
            android:keyWidth="15%p"/>
        <Key android:codes="-4" android:keyIcon="@drawable/sym_keyboard_feedback_return"
            android:keyWidth="25%p"/>

    </Row>

</Keyboard>