<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="@dimen/key_height">
    <Row android:keyboardMode="@integer/keyboard_normal">
        <Key
            android:codes="97"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ver_caps_a"
             />
        <!--android:keyIcon="@drawable/alpha_cap_a_vertical"-->
        <Key
            android:codes="101"
            android:keyIcon="@drawable/alpha_cap_e_vertical" />
        <Key
            android:codes="98"
            android:keyIcon="@drawable/alpha_cap_b_vertical" />
        <Key
            android:codes="102"
            android:keyIcon="@drawable/alpha_cap_f_vertical" />
        <Key
            android:codes="103"
            android:keyIcon="@drawable/alpha_cap_g_vertical" />
        <Key
            android:codes="108"
            android:keyIcon="@drawable/alpha_cap_l_vertical" />
        <Key
            android:codes="109"
            android:keyIcon="@drawable/alpha_cap_m_vertical" />
        <Key
            android:codes="114"
            android:keyIcon="@drawable/alpha_cap_r_vertical" />
        <Key
            android:codes="118"
            android:keyIcon="@drawable/alpha_cap_v_vertical" />
        <Key
            android:codes="119"
            android:keyIcon="@drawable/alpha_cap_w_vertical" />
    </Row>

    <Row android:keyboardMode="@integer/keyboard_normal">
        <Key
            android:codes="105"
            android:horizontalGap="5%"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/alpha_cap_i_vertical" />
        <Key
            android:codes="111"
            android:keyIcon="@drawable/alpha_cap_o_vertical" />
        <Key
            android:codes="99"
            android:keyIcon="@drawable/alpha_cap_c_vertical" />
        <Key
            android:codes="104"
            android:keyIcon="@drawable/alpha_cap_h_vertical" />
        <Key
            android:codes="110"
            android:keyIcon="@drawable/alpha_cap_n_vertical" />
        <Key
            android:codes="112"
            android:keyIcon="@drawable/alpha_cap_p_vertical" />
        <Key
            android:codes="115"
            android:keyIcon="@drawable/alpha_cap_s_vertical" />
        <Key
            android:codes="120"
            android:keyIcon="@drawable/alpha_cap_x_vertical" />
        <Key
            android:codes="121"
            android:keyIcon="@drawable/alpha_cap_y_vertical" />
    </Row>

    <Row
        android:keyBackground="@drawable/background_oped_orange"
        android:keyboardMode="@integer/keyboard_normal">
        <Key
            android:codes="-1"
            android:isSticky="true"
            android:horizontalGap="2%"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/capson_icon"
            android:keyWidth="12%p" />

        <Key
            android:codes="117"
            android:keyIcon="@drawable/alpha_cap_u_vertical" />
        <Key
            android:codes="100"
            android:keyIcon="@drawable/alpha_cap_d_vertical" />
        <Key
            android:codes="106"
            android:keyIcon="@drawable/alpha_cap_j_vertical" />
        <Key
            android:codes="107"
            android:keyIcon="@drawable/alpha_cap_k_vertical" />
        <Key
            android:codes="113"
            android:keyIcon="@drawable/alpha_cap_q_vertical" />
        <Key
            android:codes="116"
            android:keyIcon="@drawable/alpha_cap_t_vertical" />
        <Key
            android:codes="122"
            android:keyIcon="@drawable/alpha_cap_z_vertical" />
        <Key
            android:codes="-5"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="12%p" />
    </Row>
    <Row android:keyboardMode="@integer/keyboard_symbol">
        <Key
            android:codes="49"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:keyLabel="3" />
        <Key
            android:codes="52"
            android:keyBackground="@drawable/background_oped_orange"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:keyLabel="6" />
        <Key
            android:codes="55"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:keyLabel="9" />
        <Key
            android:codes="48"
            android:keyLabel="0" />
    </Row>

    <Row android:keyboardMode="@integer/keyboard_symbol">
        <Key
            android:codes="33"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="!" />
        <Key
            android:codes="34"
            android:keyLabel="&quot;" />
        <Key
            android:codes="39"
            android:keyLabel="'" />
        <Key
            android:codes="58"
            android:keyLabel=":" />
        <Key
            android:codes="59"
            android:keyLabel=";" />
        <Key
            android:codes="47"
            android:keyLabel="/" />
        <Key
            android:codes="63"
            android:keyLabel="\?" />
        <Key
            android:codes="64"
            android:keyEdgeFlags="right"
            android:keyLabel="\@" />
        <Key
            android:codes="35"
            android:keyLabel="\#" />

    </Row>
    <Row android:keyboardMode="@integer/keyboard_symbol">


        <Key
            android:codes="36"
            android:horizontalGap="1%p"
            android:keyEdgeFlags="left"
            android:keyLabel="$" />
        <Key
            android:codes="37"
            android:keyLabel="%" />
        <Key
            android:codes="38"
            android:keyLabel="&amp;" />
        <Key
            android:codes="42"
            android:keyLabel="*" />
        <Key
            android:codes="45"
            android:keyLabel="-" />
        <Key
            android:codes="61"
            android:keyLabel="=" />
        <Key
            android:codes="40"
            android:keyIcon="@android:drawable/ic_menu_search"
            android:keyLabel="(" />
        <Key
            android:codes="41"
            android:keyEdgeFlags="right"
            android:keyLabel=")" />
        <Key
            android:codes="-5"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="12%p" />
    </Row>


    <Row
        android:keyboardMode="@integer/keyboard_normal"
        android:rowEdgeFlags="bottom">

        <Key
            android:codes="12"
            android:keyIcon="@drawable/icons8globe"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="123"
            android:keyWidth="15%p" />

        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->

        <Key
            android:codes="32"
            android:keyIcon="@drawable/sym_keyboard_space_holo_dark"
            android:keyWidth="30%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="-4"
            android:keyIcon="@drawable/sym_keyboard_feedback_return"
            android:keyWidth="20%p" />
    </Row>
    <Row
        android:keyboardMode="@integer/keyboard_symbol"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="12"
            android:keyIcon="@drawable/icons8globe"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="ABC"
            android:keyWidth="15%p"
            android:keyboardMode="@integer/keyboard_symbol" />

        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->

        <Key
            android:codes="32"
            android:keyIcon="@drawable/sym_keyboard_space_holo_dark"
            android:keyWidth="30%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="-4"
            android:keyIcon="@drawable/sym_keyboard_feedback_return"
            android:keyWidth="20%p" />

    </Row>

</Keyboard>