<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="@dimen/key_height">
    <Row android:keyboardMode="@integer/keyboard_normal">
        <Key
            android:popupKeyboard="@xml/keyboard_popup_template"
            android:codes="97"
            android:keyIcon="@drawable/ver_a_small"
            android:keyEdgeFlags="left"
             />
        <!--android:keyIcon="@drawable/alpha_a_vertical"-->
        <Key
            android:popupKeyboard="@xml/keyboard_popup_template"
            android:codes="101"
            android:keyIcon="@drawable/ver_e_small" />
        <Key
            android:popupKeyboard="@xml/keyboard_popup_template"
            android:keyIcon="@drawable/ver_b_small"
            android:codes="98"
             />

        <!--android:keyIcon="@drawable/alpha_b_vertical"-->
        <Key
            android:popupKeyboard="@xml/keyboard_popup_template"
            android:codes="102"
            android:keyIcon="@drawable/ver_f_small" />
        <Key
            android:codes="103"
            android:keyIcon="@drawable/ver_g_small" />
        <Key
            android:codes="108"
            android:keyIcon="@drawable/ver_l_small" />
        <Key
            android:codes="109"
            android:keyIcon="@drawable/ver_m_small" />
        <Key
            android:codes="114"
            android:keyIcon="@drawable/ver_r_small" />
        <Key
            android:codes="118"
            android:keyIcon="@drawable/ver_v_small" />
        <Key
            android:codes="119"
            android:keyIcon="@drawable/ver_w_small" />
    </Row>

    <Row android:keyboardMode="@integer/keyboard_normal">
        <Key
            android:codes="105"
            android:horizontalGap="5%"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ver_i_small" />
        <Key
            android:codes="111"
            android:keyIcon="@drawable/ver_o_small" />
        <Key
            android:codes="99"
            android:keyIcon="@drawable/ver_c_small" />
        <Key
            android:codes="104"
            android:keyIcon="@drawable/ver_h_small" />
        <Key
            android:codes="110"
            android:keyIcon="@drawable/ver_n_small" />
        <Key
            android:codes="112"
            android:keyIcon="@drawable/ver_p_small" />
        <Key
            android:codes="115"
            android:keyIcon="@drawable/ver_s_small" />
        <Key
            android:codes="120"
            android:keyIcon="@drawable/ver_x_small" />
        <Key
            android:codes="121"
            android:keyIcon="@drawable/ver_y_small" />
    </Row>

    <Row
        android:keyBackground="@drawable/background_oped_orange"
        android:keyboardMode="@integer/keyboard_normal">
        <Key
            android:codes="-1"
            android:keyWidth="15%p"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/icon_caps_off"
             />

        <Key
            android:codes="117"
            android:keyIcon="@drawable/ver_u_small" />
        <Key
            android:codes="100"
            android:keyIcon="@drawable/ver_d_small" />
        <Key
            android:codes="106"
            android:keyIcon="@drawable/ver_j_small" />
        <Key
            android:codes="107"
            android:keyIcon="@drawable/ver_k_small" />
        <Key
            android:codes="113"
            android:keyIcon="@drawable/ver_q_small" />
        <Key
            android:codes="116"
            android:keyIcon="@drawable/ver_t_small" />
        <Key
            android:codes="122"
            android:keyIcon="@drawable/ver_z_small" />
        <Key
            android:codes="-5"
            android:keyWidth="15%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/icon_delete"
             />
    </Row>
    <Row android:keyboardMode="@integer/keyboard_symbol">
        <Key
            android:codes="49"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:keyLabel="3" />
        <Key
            android:codes="52"
            android:keyBackground="@drawable/background_oped_orange"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:keyLabel="6" />
        <Key
            android:codes="55"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:keyLabel="9" />
        <Key
            android:codes="48"
            android:keyLabel="0" />
    </Row>

    <Row android:keyboardMode="@integer/keyboard_symbol">
        <Key
            android:codes="33"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="!" />
        <Key
            android:codes="34"
            android:keyLabel="&quot;" />
        <Key
            android:codes="39"
            android:keyLabel="'" />
        <Key
            android:codes="58"
            android:keyLabel=":" />
        <Key
            android:codes="59"
            android:keyLabel=";" />
        <Key
            android:codes="47"
            android:keyLabel="/" />
        <Key
            android:codes="63"
            android:keyLabel="\?" />
        <Key
            android:codes="64"
            android:keyEdgeFlags="right"
            android:keyLabel="\@" />
        <Key
            android:codes="35"
            android:keyLabel="\#" />

    </Row>
    <Row android:keyboardMode="@integer/keyboard_symbol">
        <Key
            android:codes="36"
            android:keyWidth="15%p"
            android:keyEdgeFlags="left"
            android:keyLabel="$" />
        <Key
            android:codes="37"
            android:keyLabel="%" />
        <Key
            android:codes="38"
            android:keyLabel="&amp;" />
        <Key
            android:codes="42"
            android:keyLabel="*" />
        <Key
            android:codes="45"
            android:keyLabel="-" />
        <Key
            android:codes="61"
            android:keyLabel="=" />
        <Key
            android:codes="40"
            android:keyLabel="(" />
        <Key
            android:codes="41"
            android:keyEdgeFlags="right"
            android:keyLabel=")" />
        <Key
            android:codes="-5"
            android:keyEdgeFlags="right"
            android:keyWidth="15%p"
            android:keyIcon="@drawable/icon_delete"
             />
    </Row>


    <Row
        android:keyboardMode="@integer/keyboard_normal"
        android:rowEdgeFlags="bottom">

        <Key
            android:codes="12"
            android:keyIcon="@drawable/icon_choose_keyboard"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="123"
            android:keyWidth="15%p" />

        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->

        <Key
            android:codes="32"
            android:keyIcon="@drawable/icon_space_bar"
            android:keyWidth="30%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="-4"
            android:keyIcon="@drawable/icon_return"
            android:keyWidth="20%p" />
    </Row>
    <Row
        android:keyboardMode="@integer/keyboard_symbol"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="12"
            android:keyIcon="@drawable/icon_choose_keyboard"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="ABC"
            android:keyWidth="15%p"
            android:keyboardMode="@integer/keyboard_symbol" />

        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->

        <Key
            android:codes="32"
            android:keyIcon="@drawable/icon_space_bar"
            android:keyWidth="30%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="-4"
            android:keyIcon="@drawable/icon_return"
            android:keyWidth="20%p" />
    </Row>

</Keyboard>